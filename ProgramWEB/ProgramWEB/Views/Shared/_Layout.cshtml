
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <!-- basic -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- mobile metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <!-- site metas -->
    <title>Pluto - Responsive Bootstrap Admin Panel Templates</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- site icon -->
    <link rel="icon" href="~/Contents/pluto/images/fevicon.png" type="image/png" />
    <!-- bootstrap css -->
    <link rel="stylesheet" href="~/Contents/pluto/css/bootstrap.min.css" />
    <!-- site css -->
    <link rel="stylesheet" href="~/Contents/pluto/style.css" />
    <!-- responsive css -->
    <link rel="stylesheet" href="~/Contents/pluto/css/responsive.css" />
    <!-- color css -->
    <link rel="stylesheet" href="~/Contents/pluto/css/color_2.css" />
    <!-- select bootstrap -->
    <link rel="stylesheet" href="~/Contents/pluto/css/bootstrap-select.css" />
    <!-- scrollbar css -->
    <link rel="stylesheet" href="~/Contents/pluto/css/perfect-scrollbar.css" />
    <!-- custom css -->
    <link rel="stylesheet" href="~/Contents/pluto/css/custom.css" />
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond/js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="dashboard dashboard_1">
    <div class="full_container">
        <div class="inner_container">
            <!-- Sidebar  -->
            <nav id="sidebar">
                @RenderPage("~/Views/Shared/Parts/_Sidebar.cshtml")
            </nav>

            <!-- Content  -->
            <div id="content">
                <!-- topbar -->
                <div class="topbar">
                    @RenderPage("~/Views/Shared/Parts/_Topbar.cshtml")
                </div>
                <!-- dashboard inner -->
                <div class="midde_cont">
                    <div class="container-fluid">
                        @RenderBody()
                    </div>
                    <!-- footer -->
                    <div class="container-fluid">
                        @RenderPage("~/Views/Shared/Parts/_Footer.cshtml")
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- jQuery -->
    <script src="~/Contents/pluto/js/jquery.js"></script>
    <script src="~/Contents/pluto/js/popper.min.js"></script>
    <script src="~/Contents/pluto/js/bootstrap.min.js"></script>
    <!-- wow animation -->
    <script src="~/Contents/pluto/js/animate.js"></script>
    <!-- select country -->
    <script src="~/Contents/pluto/js/bootstrap-select.js"></script>
    <!-- owl carousel -->
    <script src="~/Contents/pluto/js/owl.carousel.js"></script>
    <!-- chart js -->
    <script src="~/Contents/pluto/js/Chart.min.js"></script>
    <script src="~/Contents/pluto/js/Chart.bundle.min.js"></script>
    <script src="~/Contents/pluto/js/utils.js"></script>
    <script src="~/Contents/pluto/js/analyser.js"></script>
    <!-- nice scrollbar -->
    <script src="~/Contents/pluto/js/perfect-scrollbar.min.js"></script>
    <script>
        var ps = new PerfectScrollbar('#sidebar');
    </script>
    <!-- custom js -->
    <script src="~/Contents/pluto/js/custom.js"></script>
    <script src="~/Contents/pluto/js/chart_custom_style1.js"></script>
</body>
</html>
